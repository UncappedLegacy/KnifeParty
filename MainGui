--Properties:

ScreenGui.Parent = game.CoreGui

main.Name = "main"
main.Parent = ScreenGui
main.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
main.BorderColor3 = Color3.fromRGB(255, 0, 110)
main.Position = UDim2.new(0.571544051, 0, 0.330658197, 0)
main.Size = UDim2.new(0, 444, 0, 389)
main.Active = true
main.Draggable = true

labeltitle.Name = "label title"
labeltitle.Parent = main
labeltitle.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
labeltitle.BorderSizePixel = 0
labeltitle.Size = UDim2.new(0, 444, 0, 33)
labeltitle.Font = Enum.Font.Sarpanch
labeltitle.Text = "Knife Party - Crate GUI"
labeltitle.TextColor3 = Color3.fromRGB(255, 0, 110)
labeltitle.TextSize = 14.000

labelcredit.Name = "label credit"
labelcredit.Parent = main
labelcredit.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
labelcredit.BorderSizePixel = 0
labelcredit.Position = UDim2.new(0, 0, 0.947104335, 0)
labelcredit.Size = UDim2.new(0, 444, 0, 20)
