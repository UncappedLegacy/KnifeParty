local ScreenGui = Instance.new("ScreenGui")
local main = Instance.new("Frame")
local labeltitle = Instance.new("TextLabel")
local labelcredit = Instance.new("TextLabel")
local KnifeCase3 = Instance.new("TextButton")
local KnifeCase2 = Instance.new("TextButton")
local KnifeCase1 = Instance.new("TextButton")
local GunCase2 = Instance.new("TextButton")
local GunCase1 = Instance.new("TextButton")
