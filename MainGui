KnifeCase3.Name = "KnifeCase3"
KnifeCase3.Parent = main
KnifeCase3.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
KnifeCase3.BorderSizePixel = 0
KnifeCase3.Position = UDim2.new(0.0270270295, 0, 0.314736009, 0)
KnifeCase3.Size = UDim2.new(0, 420, 0, 34)
KnifeCase3.Font = Enum.Font.SourceSans
KnifeCase3.Text = "Knife Case 3"
KnifeCase3.TextColor3 = Color3.fromRGB(255, 0, 110)
KnifeCase3.TextSize = 14.000
KnifeCase3.MouseButton1Down:connect(function()
        getgenv().farmer = true;
        while wait() do
                if getgenv().farmer == true then
                        game.ReplicatedStorage.Remotes.BuyCrate:invokeServer("0.00000000001", "Money", "KnifeCase3")
                end
        end
end)

